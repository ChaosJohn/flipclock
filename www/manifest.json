{
  "manifest_version": 2,
  "name": "FlipClock",
  "description": "Flip away the time with FlipClock.",
  "version": "4.2.0",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": {
    "16": "images/logos/FlipClock_16.png",
    "128": "images/logos/FlipClock_128.png"
  },
  "permissions": ["storage", "notifications"]
}

